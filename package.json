{
  "name": "cmr-electron-demo",
  "version": "1.0.0",
  "description": "A sandbox Electron application",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-windows-x64": "electron-packager . --platform=win32 --arch=x64",
    "package-osx": "electron-packager . --platform=darwin --arch=x64 --extra-resource=gogen"
  },
  "repository": "https://github.com/codeforamerica/cmr-electron-demo",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "sandbox",
    "demo"
  ],
  "author": "Code for America",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^4.1.3",
    "electron-packager": "^13.1.1"
  }
}
